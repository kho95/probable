{
  "name": "gatsby-kick-starter",
  "private": true,
  "description": "A Gatsby kick starter, using Typescript and Styled Components",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "start": "run-p start:**",
    "start:app": "npm run develop",
    "start:lambda": "netlify-lambda serve src/lambda",
    "build": "run-p build:**",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build src/lambda",
    "format": "prettier --write src/**/*.{js,jsx}",
    "serve": "gatsby serve",
    "lint": "eslint --ext=js,jsx,ts,tsx src",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "type-check": "tsc"
  },
  "dependencies": {
    "@types/lodash": "^4.14.148",
    "@types/lodash.merge": "^4.6.6",
    "axios": "^0.19.2",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "fast-xml-parser": "^3.16.0",
    "gatsby": "^2.24.1",
    "gatsby-plugin-create-client-paths": "^2.1.22",
    "gatsby-plugin-manifest": "^2.2.41",
    "gatsby-plugin-netlify": "^2.1.32",
    "gatsby-plugin-polished": "^1.0.4",
    "gatsby-plugin-styled-components": "^3.1.19",
    "gatsby-plugin-typescript": "^2.1.27",
    "html-react-parser": "^0.9.1",
    "json2csv": "^4.5.4",
    "lodash": "^4.17.19",
    "lodash.merge": "^4.6.2",
    "moment": "^2.24.0",
    "polished": "^3.4.4",
    "prismic-javascript": "^2.1.5",
    "react": "^16.8.6",
    "react-animations": "^1.0.0",
    "react-dates": "^21.8.0",
    "react-dom": "^16.8.6",
    "react-router-hash-link": "^2.0.0",
    "react-table": "^7.0.0-rc.15",
    "styled-components": "^4.3.2",
    "styled-icons": "^10.11.0"
  },
  "devDependencies": {
    "@svgr/cli": "^4.3.3",
    "@testing-library/react": "^9.1.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.26",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.0",
